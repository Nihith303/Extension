{
  "manifest_version": 3,
  "name": "digispot-ai",
  "version": "1.0.0",
  "description": "SEO Audit of website to get Summary, Links, Images, Headings, Schema, and more.",
  "action": {
    "default_popup": "popup.html",
    "default_title": "digspot-ai",
    "default_icon": {
      "16": "image/icon16.png"
    }
  },
  "icons": {
    "16": "image/icon16.png"
  },
  "permissions": ["activeTab", "scripting", "storage", "tabs"],
  "host_permissions": ["<all_urls>"]
}
